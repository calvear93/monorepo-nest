{
	"|DEFAULT|": {
		"TZ": "America/Santiago",
		"LANG": "en-US",
		"TARGET": "es2022",
		"SWAGGER_UI": true,
		"API_PREFIX": "api"
	},
	"|MODE|": {
		"build": {
			"NODE_ENV": "production",
			"PORT": 8080,
			"API_PREFIX": "api/(((base-path)))"
		},
		"debug": {
			"NODE_ENV": "development",
			"#DEBUG": "express:*",
			"PORT": 4001
		},
		"test": {
			"NODE_ENV": "test"
		},
		"@apps/core": {
			"API_PREFIX": "api/core"
		}
	}
}
